// import java.util.Stack;

class StockSpanner {
    public Stack<int[]> stack;

    public StockSpanner() {
        stack = new Stack<>();
    }

    public int next(int price) {
        int sp= 1;
        while (!stack.isEmpty() && stack.peek()[0] <= price) {
            sp += stack.pop()[1]; 
        }
        stack.push(new int[]{price, sp}); 
        return sp;
    }
}
