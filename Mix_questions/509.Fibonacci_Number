class Solution {
    public int fib(int n) {
        int [] dp=new int[n+1];
        Arrays.fill(dp,-1);
        return fab(n,dp);
        
    }
    public int fab(int n,int [] dp){
        if(n<=1)return n;
        if(dp[n]!=-1)return dp[n];
        dp[n]=fab(n-1,dp)+fab(n-2,dp);
        return dp[n];

    }
}
