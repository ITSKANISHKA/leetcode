class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int n=matrix.length;
        int m=matrix[0].length;
        int r=0;
        int col=m-1;
        while(r<n && col>=0){
            if(matrix[r][col]==target){
                return true;
            }if(matrix[r][col]<target){
                r++;
            }else{
                col--;
            }
        }return false;
        
    }
}
